---
  parser: "babel-eslint"
  rules: 
    indent: 
      - 2
      - 2
    quotes: 
      - 2
      - "single"
    key-spacing: 
      - 2
      - 
        align: "colon"
    linebreak-style: 
      - 2
      - "unix"
    semi: 
      - 2
      - "always"
    no-alert: 1
    no-console: 1
    no-var: 2
    no-lonely-if: 2
    new-cap: 0
    object-shorthand:
      - 2
      - "always"
  globals:
    createjs: true
  env:
    es6: true
    browser: true
    jquery: true
  ecmaFeatures:
    modules: true
  plugins: []

